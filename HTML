<html>
<head>
<script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
  </script>
  <script>    
    
    function swapImages(){
      var $active = $('#myGallery .active');
      var $next = ($('#myGallery .active').next().length > 0) ? $('#myGallery .active').next() : $('#myGallery img:first');

      $next.fadeIn(function(){         
        $active.removeClass('active');
        $next.addClass('active');
        $active.fadeOut();
      })
            
    }

    $(document).ready(function(){
      
      var height = $( document ).height();
      var width = $( document ).width();      
      var size = width + ',' + height;
      
      var locations = [        
 
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=FHA"},
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=Conventional"},
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=Flex"},
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=FHA%20Plus"},
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=FHA%20Plus%20Second"},
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=Refinance"},
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=FNMA"},
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=Plus%20Second%20Mortgage"},
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=Rural%20Development"},
        {"src": "https://analytics.vhda.com/t/AuditRisk/views/Zach/Dashboard1?&:embed=yes&:toolbar=no&:format=png&:size="+size+"&Prod=VA"},
     
                      ]
            
      $("#myGallery").attr('width',width);
      $("#myGallery").attr('height',height);
      
      var img = '';
      
      locations.forEach(function(element, index, array){
        
        
        if(index==0){         
          img = '<img src="'+locations[index].src+'" class="active" />';
        } else {
          img = '<img src="'+locations[index].src+'" />';         
        }

        console.log('index: '+index+' == '+img);    
        
        $(img).appendTo("#myGallery");
        
      })
      
      
      // Change 3000 to whatever time interval you prefer
      setInterval('swapImages()', 3000);
    });
  </script>
  <style>
    
    body {
      background-color: #353535;
    }
    #myGallery{
      position:relative;
    }
    #myGallery img{
      display:none;
      position:absolute;
      top:0;
      left:0;
    }
    #myGallery img.active{
      display:block;
    }
  </style>
